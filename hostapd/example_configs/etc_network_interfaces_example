# interfaces(5) file used by ifup(8) and ifdown(8)
auto lo
iface lo inet loopback

auto eth0
iface eth0 inet dhcp

auto eth1
iface eth1 inet manual

auto br0
allow-ovs br0
iface br0 inet manual
	ovs_type OVSBridge
	ovs_ports eth2 eth3 eth4 eth5 eth6 eth7 eth8 eth9 wlan0

allow-br0 eth2
iface eth2 inet manual
	ovs_bridge br0
	ovs_type OVSPort

allow-br0 eth3
iface eth3 inet manual
	ovs_bridge br0
	ovs_type OVSPort

allow-br0 eth4
iface eth4 inet manual
	ovs_bridge br0
	ovs_type OVSPort

allow-br0 eth5
iface eth5 inet manual
	ovs_bridge br0
	ovs_type OVSPort

allow-br0 eth6
iface eth6 inet manual
	ovs_bridge br0
	ovs_type OVSPort

allow-br0 eth7
iface eth7 inet manual
	ovs_bridge br0
	ovs_type OVSPort

allow-br0 eth8
iface eth8 inet manual
	ovs_bridge br0
	ovs_type OVSPort

allow-br0 eth9
iface eth9 inet manual
	ovs_bridge br0
	ovs_type OVSPort

allow-br0 wlan0
iface wlan0 inet manual
	ovs_bridge br0
	ovs_type OVSPort
#wireless-mode Master
